{
  "name": "gpu-tex-enc",
  "version": "1.2.4",
  "description": "API and CLI for compressing images using the ASTC, ETC, ETC2, BCn (DXTn) and BASIS texture compression standard.",
  "bin": {
    "gputexenc": "./src/cli.js"
  },
  "main": "./src/index.js",
  "types": "./src/types.d.ts",
  "keywords": [
    "compression",
    "image-compression",
    "astc",
    "astcenc",
    "etc1",
    "etc2",
    "etc2comp",
    "EtcTool",
    "bc1",
    "bc2",
    "bc3",
    "bc4",
    "bc5",
    "dxt1",
    "dxt3",
    "dxt5",
    "bc7",
    "bc7enc",
    "uastc",
    "etc1s",
    "basis",
    "GPUTexEncoder"
  ],
  "author": "Denys Denysiuk <denisyuk.d@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@gpu-tex-enc/astc": "^4.7.0",
    "@gpu-tex-enc/bc": "^1.0.11",
    "@gpu-tex-enc/etc": "^1.0.3",
    "@gpu-tex-enc/basis": "^1.16.3",
    "yargs": "^17.7.2"
  },
  "optionalDependencies": {
    "sharp": "^0.33.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "release": "npm publish --access public"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ddenisyuk/gpu-tex-enc.git"
  }
